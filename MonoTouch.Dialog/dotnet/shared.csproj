<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <RootNamespace>MonoTouch.Dialog</RootNamespace>
    <AssemblyName>MonoTouch.Dialog</AssemblyName>
    <IPhoneResourcePrefix>Resources</IPhoneResourcePrefix>
    <RootDirectory>$(MSBuildThisFileDirectory)/../</RootDirectory>

    <!-- warning CA1422: This call site is reachable on: '...' unsupported on: '...': MonoTouch.Dialog isn't really maintained, so we're not removing usage of unsupported APIs unless we really have to, so just ignore any such warnings -->
    <NoWarn>$(NoWarn);CA1416</NoWarn>

    <!-- warning CA1422: This call site is reachable on: '...' obsoleted on: '...': MonoTouch.Dialog isn't really maintained, so we're not removing usage of obsoleted APIs unless we really have to, so just ignore any such warnings -->
    <NoWarn>$(NoWarn);CA1422</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="$(RootDirectory)Elements.cs" />
    <Compile Include="$(RootDirectory)DialogViewController.cs" />
    <Compile Include="$(RootDirectory)Reflect.cs" />
    <Compile Include="$(RootDirectory)Utilities\Graphics.cs" />
    <Compile Include="$(RootDirectory)OldElements\LoadMoreElement.cs" />
    <Compile Include="$(RootDirectory)Utilities\ImageLoader.cs" />
    <Compile Include="$(RootDirectory)Utilities\LRUCache.cs" />
    <Compile Include="$(RootDirectory)Elements\MessageElement.cs" />
    <Compile Include="$(RootDirectory)Elements\OwnerDrawnElement.cs" />
    <Compile Include="$(RootDirectory)Elements\ActivityElement.cs" />
    <Compile Include="$(RootDirectory)Elements\ElementBadge.cs" />
    <Compile Include="$(RootDirectory)Utilities\Controls.cs" />
    <Compile Include="$(RootDirectory)Utilities\GlassButton.cs" />
    <Compile Include="$(RootDirectory)Utilities\LocalizationExtensions.cs" />
    <Compile Include="$(RootDirectory)Elements\Json.cs" />
  </ItemGroup>
</Project>
